{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "//items/final_release",
  "type": "object",
  "title": "The final release item",
  "required": [
    "header"
  ],
  "additionalProperties": false,
  "properties": {
    "header": {
      "$ref": "./header.json"
    },
    "cargo": {
      "$ref": "./cargo.json"
    },
    "customsdeclaration": {
      "$id": "#/properties/customsdeclaration",
      "type": "array",
      "title": "The list of custom declarations",
      "items": {
        "$ref": "./declaration.json"
      }
    },
    "exportRoute": {
      "$ref": "./route.json"
    },
    "traders": {
      "$id": "#/properties/traders",
      "type": "array",
      "title": "The list of traders",
      "items": {
        "$ref": "./trader.json"
      }
    }
  }
}
