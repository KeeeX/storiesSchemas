{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "//items/image",
  "type": "object",
  "title": "The image item",
  "required": [
    "bookingReference",
    "reference"
  ],
  "additionalProperties": true,
  "properties": {
    "reference": {
      "$id": "#/properties/reference",
      "type": "string",
      "title": "The image reference"
    },
    "bookingReference": {
      "$id": "#/properties/bookingReference",
      "type": "string",
      "title": "The booking reference"
    },
    "imageType": {
      "$id": "#/properties/imageType",
      "type": "string",
      "title": "The image type"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The file name"
    }
  }
}
