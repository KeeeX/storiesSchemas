{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "//items/container",
  "type": "object",
  "title": "The container item",
  "required": [
    "bookingReference",
    "number"
  ],
  "additionalProperties": true,
  "properties": {
    "bookingReference": {
      "$id": "#/properties/bookingReference",
      "type": "string",
      "title": "The booking reference"
    },
    "number": {
      "$id": "#/properties/number",
      "type": "string",
      "title": "The container number"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The file name"
    }
  }
}
