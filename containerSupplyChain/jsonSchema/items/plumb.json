{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "//items/plumb",
  "type": "object",
  "title": "The plumb item",
  "required": [
    "bookingReference",
    "containerNumber",
    "reference"
  ],
  "additionalProperties": true,
  "properties": {
    "bookingReference": {
      "$id": "#/properties/bookingReference",
      "type": "string",
      "title": "The booking reference"
    },
    "containerNumber": {
      "$id": "#/properties/containerNumber",
      "type": "string",
      "title": "The container number"
    },
    "reference": {
      "$id": "#/properties/reference",
      "type": "string",
      "title": "The plumb reference"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The file name"
    }
  }
}
